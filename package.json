  "license": "MIT",
  "dependencies": {
    "graceful-fs": "^4.1.11",
    "karma": "1.X >= 0.13.X",
    "lodash": "^4.17.4",
    "yargs": "^6.6.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "karma-mocha": "1.3.0",
    "karma-phantomjs-launcher": "1.0.2",
    "mocha": "^3.2.0",
    "rewire": "^2.5.2",
    "sinon": "^1.17.7"
  }
}
